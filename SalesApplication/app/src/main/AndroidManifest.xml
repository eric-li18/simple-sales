<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.b07">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity android:name=".store.login.StoreAuthenticationActivity" />
    <activity android:name=".store.signup.AccountCreationActivity" />
    <activity android:name=".store.signup.WelcomeActivity" />
    <activity android:name=".store.customer.CustomerUIActivity" />
    <activity android:name=".store.admin.AdminUIActivity" />
    <activity android:name=".store.employee.EmployeeUIActivity" />
  </application>

</manifest>